{
    "version": 2,
    "builds": [
      {
        "src": "src/middleware/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "src/pages/**/*",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/middleware/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/pages/$1"
      }
    ]
  }
  